<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".controllers.activities.SuperMasterActivity">


    <android.support.v7.widget.Toolbar
        android:id="@+id/activity_super_master_setting_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        style="@style/Toolbar">



    </android.support.v7.widget.Toolbar>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_normal"
        android:layout_marginTop="@dimen/margin_normal"
        android:text="최고관리자 목록"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/activity_master_setting_top_master"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="계정삭제요청 목록"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/activity_master_setting_delete_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="멤버추방요청 목록"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/activity_master_setting_delete_member"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="코인수정요청 목록"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/activity_master_setting_charge_coin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

</LinearLayout>
